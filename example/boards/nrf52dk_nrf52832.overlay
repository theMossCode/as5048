&uart0 {
    status = "okay";
    current-speed = < 115200 >;
};


&spi0 {
    status = "okay";
    cs-gpios = <&gpio0 25 GPIO_ACTIVE_LOW>;
    as5048a@0 {
        status = "okay";
        compatible = "ams,as5048a";
        reg = < 0x00 >;
        spi-max-frequency = < 200000 >;
        label = "as5048a";
    };
};
